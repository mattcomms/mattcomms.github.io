{
  "name": "Mattcomms UX",
  "description": "Mattcomms UX theme local config file.",
  "css": {
    "file": "style.scss",
    "src": [
      "./src/scss/**/*.scss"
    ],
    "dest": "css"
  },
  "js": {
    "file": "scripts.js",
    "src": [
      "./src/js/*.js"
    ],
    "dest": "js"
  },
  "uikit_js": {
    "src": [
      "./src/uikit/js/*.js"
    ],
    "dest": "js"
  },
  "images": {
    "src": [
      "./src/images/**/*"
    ],
    "dest": "images"
  }
}
